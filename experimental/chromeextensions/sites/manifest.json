

{
  "manifest_version": 2,

  "name": "My extenion to get rid of annoyances",
  "description": "Multiple sites",
  "version": "0.1",
  "icons": { "16": "icon/icon16.png",
           "48": "icon/icon48.png",
          "128": "icon/icon128.png" },

  "browser_action": {
    "default_icon": "icon/icon.png"
  },
  "permissions": [
    "activeTab"
  ],

  "content_scripts": [
    {
      "matches": [
        "http://www.watchseries.ac/*"
      ],
      "css": ["watchseries/mycss.css"]
    }
  ]
}
